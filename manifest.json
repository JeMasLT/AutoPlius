{
    "manifest_version": 2,

    "name": "AutoPlius contact collector",
    "description": "Patogesnis budas surinkti kontaktus kuriems nori skambinti",
    "version": "1.0",

    "browser_action": {
        "default_icon": "img/icon48.png"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
    {
      "matches": ["http://auto.plius.lt/*", "https://auto.plius.lt/*"],
      "js": ["background.js"]
    }
  ]
}